<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


    <!-- Services -->
    <bean id="carpoolServiceImpl" class="com.kasun.transport.carpool.services.CarpoolServiceImpl">
        <property name="carpoolCreationLogic" ref="carpoolCreationLogic"/>
    </bean>

    <!-- Logic -->
    <bean id="carpoolCreationLogic" class="com.kasun.transport.carpool.logic.CarpoolCreationLogic">
    </bean>

</beans>